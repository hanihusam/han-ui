{
  "name": "@hanihusam/han-core",
  "version": "0.1.3-canary.0",
  "description": "Core theme variables for the Han UI Design System.",
  "han:src": "./src/index.ts",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/icons.esm.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/hanihusam/han-ui.git",
  "author": "Hani Husamuddin <hani.husam@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdx build --transpileOnly --tsconfig tsconfig.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublish": "yarn build",
    "start": "tsdx watch --tsconfig tsconfig.json --verbose",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "polished": "^4.1.3"
  },
  "gitHead": "eba974b096a040c2f388e753a32e3673f9b5f1a7"
}
